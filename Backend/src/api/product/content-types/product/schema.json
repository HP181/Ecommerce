{
  "kind": "collectionType",
  "collectionName": "products",
  "info": {
    "singularName": "product",
    "pluralName": "products",
    "displayName": "product",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "title": {
      "type": "string",
      "required": true,
      "maxLength": 71,
      "minLength": 3
    },
    "slug": {
      "type": "uid",
      "required": true,
      "minLength": 3,
      "maxLength": 110
    },
    "description": {
      "type": "text"
    },
    "image": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": [
        "images"
      ]
    },
    "category": {
      "type": "enumeration",
      "enum": [
        "Men's Clotches",
        "manshoes",
        "Women's Clotches",
        "Electronic Items",
        "Groceries",
        "Food Items"
      ],
      "default": "Men's Clotches",
      "required": true
    },
    "price": {
      "type": "integer",
      "required": true,
      "default": 0
    },
    "availableQty": {
      "type": "integer",
      "required": true,
      "default": 0
    },
    "color": {
      "type": "json",
      "required": true
    },
    "size": {
      "type": "json",
      "required": true
    }
  }
}
